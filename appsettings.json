{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "MySequelConnection": "${MY_SEQUEL_CONNECTION}"
    },
    "ServerConfiguration": {
        "ClientOrigin": "https://schedule.miloszgilga.pl",
        "DevClientOrigin": "localhost:8383",
        "JwtKey": "${JWT_KEY}",
        "JwtExpiredTimestamp": "00:24:00",
        "DbDriverVersion": "8.0.29",
        "SshUsername": "${SSH_USERNAME}",
        "SshPassword": "${SSH_PASSWORD}",
        "SshPasswordFieldName": "Password",
        "SshServer": "${SSH_SERVER}",
        "UserEmailDomain": "schedule.miloszgilga.pl",
        "UserEmailMaxSizeMb": 20,
        "OptExpired": "00:10:00",
        "SmtpSenderAddress": "noreply@schedule.miloszgilga.pl",
        "SmtpSenderDisplayName": "System ZarzÄ…dzania Planem",
        "SmtpUsername": "${SMTP_USERNAME}",
        "SmtpPassword": "${SMTP_PASSWORD}",
        "SmtpHost": "${SMTP_HOST}",
        "SmtpPort": 587,
        "EnableSSL": true,
        "UseDefaultCredentials": true,
        "IsBodyHTML": true,
        "InitialCredentials": {
            "AccountName": "${INITIAL_ACCOUNT_NAME}",
            "AccountSurname": "${INITIAL_ACCOUNT_SURNAME}",
            "AccountPassword": "${INITIAL_ACCOUNT_PASSWORD}"
        }
    }
}